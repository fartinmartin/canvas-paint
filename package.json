{
	"name": "canvas-paint",
	"version": "0.0.0",
	"type": "module",
	"files": [
		"dist"
	],
	"types": "./dist/types",
	"main": "./dist/canvas-paint.umd.cjs",
	"module": "./dist/canvas-paint.js",
	"exports": {
		".": {
			"import": "./dist/canvas-paint.js",
			"require": "./dist/canvas-paint.umd.cjs"
		}
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build && npm run types",
		"preview": "vite preview",
		"build:demo": "vite build --config vite.demo.js",
		"preview:demo": "vite preview --config vite.demo.js",
		"types": "npx tsc --noEmit false --declaration --emitDeclarationOnly --declarationDir dist/types"
	},
	"devDependencies": {
		"@types/lazy-brush": "^1.0.1",
		"@types/lodash-es": "^4.17.6",
		"typescript": "^4.6.4",
		"vite": "^3.1.0"
	},
	"dependencies": {
		"catenary-curve": "^1.0.1",
		"color2k": "^2.0.0",
		"combinate": "^1.1.11",
		"culori": "^2.0.3",
		"lazy-brush": "^1.0.1",
		"lodash-es": "^4.17.21",
		"q-floodfill": "^1.3.1",
		"resize-observer-polyfill": "^1.5.1"
	}
}
